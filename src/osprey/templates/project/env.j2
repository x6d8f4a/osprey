# {{ project_name }} Environment Configuration

# API Keys
{% if env.get('CBORG_API_KEY') -%}
CBORG_API_KEY={{ env.CBORG_API_KEY }}
{% endif -%}
{% if env.get('AMSC_API_KEY') -%}
AMSC_API_KEY={{ env.AMSC_API_KEY }}
{% endif -%}
{% if env.get('OPENAI_API_KEY') -%}
OPENAI_API_KEY={{ env.OPENAI_API_KEY }}
{% endif -%}
{% if env.get('ANTHROPIC_API_KEY') -%}
ANTHROPIC_API_KEY={{ env.ANTHROPIC_API_KEY }}
{% endif -%}
{% if env.get('GOOGLE_API_KEY') -%}
GOOGLE_API_KEY={{ env.GOOGLE_API_KEY }}
{% endif -%}
{% if env.get('ARGO_API_KEY') -%}
ARGO_API_KEY=$${USER}
{% endif -%}
{% if env.get('STANFORD_API_KEY') -%}
STANFORD_API_KEY={{ env.STANFORD_API_KEY }}
{% endif -%}

# Project Configuration (Optional - for advanced use cases)
# Uncomment if you need to override the project root set in config.yml
# Useful for multi-environment deployments or if you move the project directory
{% if env.get('PROJECT_ROOT') -%}
PROJECT_ROOT={{ env.PROJECT_ROOT }}
{% else -%}
#PROJECT_ROOT={{ project_root }}
{% endif -%}
{% if env.get('LOCAL_PYTHON_VENV') -%}
LOCAL_PYTHON_VENV={{ env.LOCAL_PYTHON_VENV }}
{% else -%}
#LOCAL_PYTHON_VENV={{ current_python_env }}
{% endif -%}

# Timezone
TZ={{ env.get('TZ', 'America/Los_Angeles') }}
